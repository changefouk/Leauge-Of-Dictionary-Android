<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.leaugeofdictionarys">
    <application
        android:allowBackup="true"
        android:icon="@drawable/llllllll"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.asus.leaugeofdictionarys.TitleScreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.asus.leaugeofdictionarys.MainActivity"
            android:label="Search"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.example.asus.leaugeofdictionarys.DetailVocab"
            android:label="Total Word"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.example.asus.leaugeofdictionarys.AboutUs"
            android:label="About US"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.example.asus.leaugeofdictionarys.VocabActivity"
            android:label="Vocab"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>


    </application>

</manifest>
